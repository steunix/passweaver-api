{
  "master_key_file": "none",
  "jwt_duration": "24h",
  "automated_test": true,
  "listen": {
    "port": 3000,
    "host": "localhost"
  },
  "log": {
    "dir": "./log",
    "rotation": "1d",
    "retention": 14
  },
  "https": {
    "enabled": false
  },
  "redis": {
    "enabled": true,
    "url": "redis://localhost:6379"
  },
  "onetimetokens": {
    "max_hours": 168
  },
  "crypto": {
    "personal_key_pbkdf2_iterations": 100000
  },
  "readonly": false,
  "enable_metrics": true
}
